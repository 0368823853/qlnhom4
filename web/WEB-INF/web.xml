<?xml version="1.0" encoding="UTF-8"?>
<web-app version="6.0" xmlns="https://jakarta.ee/xml/ns/jakartaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="https://jakarta.ee/xml/ns/jakartaee https://jakarta.ee/xml/ns/jakartaee/web-app_6_0.xsd">
    <servlet>
        <servlet-name>EmployeServlet</servlet-name>
        <servlet-class>ControllerEmployee.EmployeServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Emp_NhanVienServlet</servlet-name>
        <servlet-class>ControllerEmployee.Emp_NhanVienServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Emp_AddNhanVien</servlet-name>
        <servlet-class>DAO.Emp_AddNhanVien</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>EmployeServlet</servlet-name>
        <url-pattern>/employe</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Emp_NhanVienServlet</servlet-name>
        <url-pattern>/nhanvienservlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Emp_AddNhanVien</servlet-name>
        <url-pattern>/addnhanvien</url-pattern>
    </servlet-mapping>
</web-app>
